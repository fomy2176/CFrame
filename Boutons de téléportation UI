local Character = game.Players.LocalPlayer.Character --Obtenir le character du joueur
script.Parent["1"].MouseButton1Click:Connect(function() --function 1 click
	Character.HumanoidRootPart.CFrame = CFrame.new(--Position--) --Teleporter
	workspace["1"].Highlight.Enabled = true --Activer Highlight
end) --fin de la 1ème fonction
script.Parent["2"].MouseButton1Click:Connect(function() --function 1 click
	Character.HumanoidRootPart.CFrame = CFrame.new(--Position--) --Teleporter
	workspace["2"].Highlight.Enabled = true --Activer Highlight
end) --fin de la 2ème fonction
